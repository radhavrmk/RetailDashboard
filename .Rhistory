calcConfInterval =  function(data, alpha){
mu = mean(data)
sigma = sd(data)/sqrt(length(data))
left =  mu - (qnorm(1-(alpha/2)) * sigma)
right =  mu + (qnorm(1-(alpha/2)) * sigma)
return(c(left,right))
}
